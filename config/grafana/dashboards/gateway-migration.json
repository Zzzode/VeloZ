{
  "dashboard": {
    "title": "VeloZ Gateway Migration Dashboard",
    "uid": "veloz-gateway-migration",
    "tags": ["veloz", "gateway", "migration"],
    "timezone": "browser",
    "refresh": "10s",
    "panels": [
      {
        "title": "Traffic Split",
        "type": "gauge",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(veloz_gateway_requests_total{gateway=\"cpp\"}[1m])) / sum(rate(veloz_gateway_requests_total[1m])) * 100",
            "legendFormat": "C++ Gateway %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 95}
              ]
            }
          }
        }
      },
      {
        "title": "Request Rate",
        "type": "timeseries",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(veloz_gateway_requests_total{gateway=\"cpp\"}[1m]))",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "sum(rate(veloz_gateway_requests_total{gateway=\"python\"}[1m]))",
            "legendFormat": "Python Gateway"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(veloz_gateway_requests_total{gateway=\"cpp\",status=~\"5..\"}[5m])) / sum(rate(veloz_gateway_requests_total{gateway=\"cpp\"}[5m])) * 100",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "sum(rate(veloz_gateway_requests_total{gateway=\"python\",status=~\"5..\"}[5m])) / sum(rate(veloz_gateway_requests_total{gateway=\"python\"}[5m])) * 100",
            "legendFormat": "Python Gateway"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 5,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1}
              ]
            }
          }
        }
      },
      {
        "title": "P50 Latency",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(veloz_gateway_request_duration_seconds_bucket{gateway=\"cpp\"}[5m])) by (le)) * 1000000",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(veloz_gateway_request_duration_seconds_bucket{gateway=\"python\"}[5m])) by (le)) * 1000000",
            "legendFormat": "Python Gateway"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "\u00b5s"
          }
        }
      },
      {
        "title": "P99 Latency",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(veloz_gateway_request_duration_seconds_bucket{gateway=\"cpp\"}[5m])) by (le)) * 1000",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(veloz_gateway_request_duration_seconds_bucket{gateway=\"python\"}[5m])) by (le)) * 1000",
            "legendFormat": "Python Gateway"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {"x": 6, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"veloz_gateway_cpp\"}[1m]) * 100",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "rate(process_cpu_seconds_total{job=\"veloz_gateway_python\"}[1m]) * 100",
            "legendFormat": "Python Gateway"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100
          }
        }
      },
      {
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"veloz_gateway_cpp\"} / 1024 / 1024",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"veloz_gateway_python\"} / 1024 / 1024",
            "legendFormat": "Python Gateway"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 200},
                {"color": "red", "value": 500}
              ]
            }
          }
        }
      },
      {
        "title": "SSE Connections",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "veloz_gateway_sse_connections{gateway=\"cpp\"}",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "veloz_gateway_sse_connections{gateway=\"python\"}",
            "legendFormat": "Python Gateway"
          }
        ]
      },
      {
        "title": "Order Processing Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(veloz_gateway_order_submitted_total{gateway=\"cpp\"}[1m]))",
            "legendFormat": "C++ - Submitted"
          },
          {
            "expr": "sum(rate(veloz_gateway_order_filled_total{gateway=\"cpp\"}[1m]))",
            "legendFormat": "C++ - Filled"
          },
          {
            "expr": "sum(rate(veloz_gateway_order_cancelled_total{gateway=\"cpp\"}[1m]))",
            "legendFormat": "C++ - Cancelled"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "title": "Health Check Status",
        "type": "stat",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "up{job=\"veloz_gateway_cpp\"}",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "up{job=\"veloz_gateway_python\"}",
            "legendFormat": "Python Gateway"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                }
              }
            ]
          }
        }
      },
      {
        "title": "Authentication Failures",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(veloz_gateway_auth_failures_total{gateway=\"cpp\"}[5m])",
            "legendFormat": "C++ Gateway"
          },
          {
            "expr": "rate(veloz_gateway_auth_failures_total{gateway=\"python\"}[5m])",
            "legendFormat": "Python Gateway"
          }
        ]
      },
      {
        "title": "Request Duration Heatmap",
        "type": "heatmap",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(veloz_gateway_request_duration_seconds_bucket{gateway=\"cpp\"}[5m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      }
    ],
    "schemaVersion": 38,
    "version": 1
  }
}
